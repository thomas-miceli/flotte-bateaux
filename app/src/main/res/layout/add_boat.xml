<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Activities.AddBoat">


    <TextView
            android:text="captain name"
            android:layout_width="134dp"
            android:layout_height="21dp"
            android:id="@+id/captainName"
            app:layout_constraintBottom_toTopOf="@+id/captainname" tools:ignore="MissingConstraints"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="122dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="122dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="16dp"/>
    <TextView
            android:text="boatname"
            android:layout_width="134dp"
            android:layout_height="25dp"
            android:id="@+id/boatName"
            app:layout_constraintBottom_toTopOf="@+id/boatname" tools:ignore="MissingConstraints"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/captainname"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="122dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="122dp"
            app:layout_constraintHorizontal_bias="1.0" app:layout_constraintVertical_bias="0.352"/>
    <TextView
            android:text="harbour"
            android:layout_width="134dp"
            android:layout_height="21dp"
            android:id="@+id/textView4"
            tools:ignore="MissingConstraints" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/boatname" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginBottom="9dp" app:layout_constraintBottom_toTopOf="@+id/harbourlist"/>
    <TextView
            android:text="longitude"
            android:layout_width="134dp"
            android:layout_height="21dp"
            android:id="@+id/longitude"
            app:layout_constraintBottom_toTopOf="@+id/editText8" tools:ignore="MissingConstraints"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/editText7"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="122dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="122dp"/>
    <EditText
            android:layout_width="374dp"
            android:layout_height="38dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/boatname"
            tools:ignore="MissingConstraints"
            android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.099"
            app:layout_constraintTop_toBottomOf="@+id/boatName" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/textView4"/>
    <EditText
            android:layout_width="359dp"
            android:layout_height="51dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/captainname"
            android:layout_marginTop="32dp"
            tools:ignore="MissingConstraints" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/captainName"
            app:layout_constraintVertical_bias="0.0" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.555"/>
    <Button
            android:text="add"
            android:layout_width="wrap_content"
            android:layout_height="41dp"
            android:id="@+id/addboat"
            style="@style/Widget.AppCompat.Button.Small"
            android:textAppearance="@style/TextAppearance.AppCompat.Button" android:visibility="visible"
            tools:ignore="MissingConstraints" android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/editText8" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="292dp" android:layout_marginBottom="65dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="40dp"/>
    <EditText
            android:layout_width="370dp"
            android:layout_height="44dp"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/editText7"
            app:layout_constraintTop_toBottomOf="@+id/latitude" tools:ignore="MissingConstraints"
            android:layout_marginTop="8dp" app:layout_constraintBottom_toTopOf="@+id/longitude"
            app:layout_constraintVertical_bias="0.0" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.0"
            android:layout_marginBottom="8dp"/>
    <EditText
            android:layout_width="375dp"
            android:layout_height="39dp"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/editText8"
            tools:ignore="MissingConstraints" android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginBottom="16dp" app:layout_constraintBottom_toTopOf="@+id/addboat"
            app:layout_constraintTop_toBottomOf="@+id/longitude" app:layout_constraintHorizontal_bias="0.714"/>
    <TextView
            android:text="latitude"
            android:layout_width="134dp"
            android:layout_height="21dp"
            android:id="@+id/latitude" tools:ignore="MissingConstraints" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
            app:layout_constraintTop_toBottomOf="@+id/harbourlist"
            app:layout_constraintBottom_toTopOf="@+id/editText7" android:layout_marginTop="8dp"/>
    <Spinner
            android:layout_width="228dp"
            android:layout_height="0dp"
            android:id="@+id/harbourlist" android:layout_marginTop="9dp"
            app:layout_constraintTop_toBottomOf="@+id/textView4" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/latitude" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="122dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="122dp"
            app:layout_constraintVertical_bias="0.0"/>
</android.support.constraint.ConstraintLayout>