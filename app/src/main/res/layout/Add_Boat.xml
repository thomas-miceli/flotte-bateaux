<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Activities.AddBoat">


    <TextView
            android:text="captain name"
            android:layout_width="134dp"
            android:layout_height="21dp"
            tools:layout_editor_absoluteX="16dp" android:id="@+id/textView6"
            app:layout_constraintBottom_toTopOf="@+id/captainname" tools:ignore="MissingConstraints"/>
    <TextView
            android:text="boatname"
            android:layout_width="134dp"
            android:layout_height="25dp"
            tools:layout_editor_absoluteX="16dp" android:id="@+id/textView5"
            app:layout_constraintBottom_toTopOf="@+id/boatname" tools:ignore="MissingConstraints"/>
    <TextView
            android:text="harbour"
            android:layout_width="134dp"
            android:layout_height="21dp"
            tools:layout_editor_absoluteX="16dp" android:id="@+id/textView4" android:layout_marginTop="12dp"
            app:layout_constraintTop_toBottomOf="@+id/boatname" tools:ignore="MissingConstraints"/>
    <TextView
            android:text="longitude"
            android:layout_width="134dp"
            android:layout_height="21dp"
            tools:layout_editor_absoluteX="16dp" android:id="@+id/textView3"
            app:layout_constraintBottom_toTopOf="@+id/editText8" tools:ignore="MissingConstraints"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/boatname"
            tools:layout_editor_absoluteX="16dp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteY="144dp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            tools:layout_editor_absoluteX="16dp" android:id="@+id/captainname"
            android:layout_marginTop="16dp"
            tools:ignore="MissingConstraints" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/textView5"
            app:layout_constraintVertical_bias="1.0"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            tools:layout_editor_absoluteX="16dp" android:id="@+id/harbour"
            tools:ignore="MissingConstraints"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/textView2" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView4" app:layout_constraintVertical_bias="0.0"/>
    <Button
            android:text="add"
            android:layout_width="wrap_content"
            android:layout_height="41dp"
            tools:layout_editor_absoluteX="292dp" android:id="@+id/button2"
            style="@style/Widget.AppCompat.Button.Small"
            android:textAppearance="@style/TextAppearance.AppCompat.Button" android:visibility="visible"
            tools:ignore="MissingConstraints" android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/editText8"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:ems="10"
            tools:layout_editor_absoluteX="16dp" android:id="@+id/editText7"
            app:layout_constraintTop_toBottomOf="@+id/textView2" tools:ignore="MissingConstraints"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:ems="10"
            tools:layout_editor_absoluteY="396dp" tools:layout_editor_absoluteX="16dp" android:id="@+id/editText8"
            tools:ignore="MissingConstraints"/>
    <TextView
            android:text="latitude"
            android:layout_width="134dp"
            android:layout_height="21dp" tools:layout_editor_absoluteY="284dp"
            tools:layout_editor_absoluteX="16dp" android:id="@+id/textView2" tools:ignore="MissingConstraints"/>
</android.support.constraint.ConstraintLayout>