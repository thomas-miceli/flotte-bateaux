<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="ovh.tomus.iut.flotte">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".Activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".Activities.FirstActivity" android:label="Act 1">
        </activity>
        <activity android:name=".Activities.SecondActivity" android:label="Act 2">
        </activity>
        <activity android:name=".Activities.LoginActivity" android:label="Login">
        </activity>
    </application>

</manifest>